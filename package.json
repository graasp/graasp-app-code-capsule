    "husky": "8.0.3",
    "nyc": "15.1.0",
    "prettier": "3.1.1",
    "uuid": "9.0.1",
    "vite": "5.0.7",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-istanbul": "5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/setupTests.ts",
      "src/**/*.d.ts"
    ]
  },
  "resolutions": {
    "@codemirror/state": "6.2.1"
  },
  "packageManager": "yarn@4.0.2",
  "msw": {
    "workerDirectory": "public"
  }
}
